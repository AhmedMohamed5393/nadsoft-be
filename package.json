{
  "name": "nadsoft-be",
  "version": "0.0.0",
  "description": "Backend system using Node.js and Express.js",
  "main": "dist/app.js",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "tests"
  },
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "start": "node dist/app",
    "start:dev": "tsc && node dist/app --watch",
    "start:debug": "node dist/app --debug --watch",
    "start:prod": "tsc && node dist/app",
    "lint": "tslint --project '.'",
    "lint-tests": "tslint --project './tests/.'",
    "test": "jest --coverage --detectOpenHandles",
    "mergetests": "npm run reinstall && npm run test",
    "reinstall": "( rm  package-lock.json && rm -r node_modules && rm -r dist && npm install ) ||  npm install "
  },
  "author": "Ahmed Mohamed",
  "license": "UNLICENCED",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "chokidar": "^3.5.3",
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.1",
    "pino": "^8.14.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.2",
    "@types/chokidar": "^2.1.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.8",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.1.2",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "jest": "^29.6.1",
    "node-mocks-http": "^1.12.2",
    "ts-jest": "^29.1.1",
    "tslint": "^6.1.3",
    "typescript": "^5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AhmedMohamed5393/nadsoft-be.git"
  },
  "keywords": [
    "backend"
  ],
  "bugs": {
    "url": "https://github.com/AhmedMohamed5393/nadsoft-be/issues"
  },
  "homepage": "https://github.com/AhmedMohamed5393/nadsoft-be#readme"
}
